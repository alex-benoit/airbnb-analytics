<div class="container" id="flats-container">
  <h1>Flats</h1>

  <% @flats.each do |flat| %>
    <div class="row">
      <div class="col-sm-6 col-md-4">
        <%= render 'flat', flat: flat %>
      </div>
    </div>
  <% end %>

  <%= link_to 'New Flat', new_flat_path, class: 'btn btn-outline-secondary' %>
</div>